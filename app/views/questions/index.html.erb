<h1>質問投稿一覧</h1>
<%= link_to "新規投稿する", :controller => "questions", :action => "new" %>
 <div class="questions-container">
   <% @questions.each do |t| %>
     <div class="question">
       <div><%= t.user.name %></div>
       <div><%= t.title %></div>
       <div><%= t.body %></div>
       <%= link_to "詳細へ", question_path(t.id) %>
     </div>
   <% end %> 
 </div>

 
