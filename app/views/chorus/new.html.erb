<h1>Join our Concerts!</h1>
<h3>新規投稿</h3>
<%= form_for @choru do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field:about,:size=>140 %>
  </div>

  <div class ="field"
   <%= f.label :about %>
   <%= f.text_field:about, :size=>140 %>
  </div> 
   
  <div class="field">
   <%= f.label :image %>
   <%= f.file_field :image %>
  </div>
  <%= f.submit "投稿する" %>
<% end %>
<%= link_to "投稿一覧に戻る", chorus_path %>